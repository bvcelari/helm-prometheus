    matchLabels:
      component: "pushgateway"
      app: prometheus
      release: prometheus-15-1638867347
  replicas: 1
  template:
    metadata:
      labels:
        component: "pushgateway"
        app: prometheus
        release: prometheus-15-1638867347
        chart: prometheus-15.0.1
        heritage: Helm
    spec:
